                    if (navigator.clipboard) {
                        navigator.clipboard.writeText(number).then(() => {
                            // Show temporary feedback
                            const originalBg = this.style.background;
                            this.style.background = 'rgba(0, 255, 0, 0.3)';
                            setTimeout(() => {
                                this.style.background = originalBg;
                            }, 1000);
                        });
                    }
                });
            });

            // Add progress tracking for checklists
            const checklistItems = document.querySelectorAll('.checklist li');
            checklistItems.forEach(item => {
                item.addEventListener('click', function() {
                    this.style.opacity = this.style.opacity === '0.5' ? '1' : '0.5';
                    this.style.textDecoration = this.style.textDecoration === 'line-through' ? 'none' : 'line-through';
                });
            });
        });

        // Make the guide more interactive
        let completedTasks = 0;
        const totalTasks = document.querySelectorAll('.checklist li').length;

        function updateProgress() {
            const progressPercent = Math.round((completedTasks / totalTasks) * 100);
