        // Simple, direct print function
        window.onload = function() {
            // Add print functionality to buttons
            const printButtons = document.querySelectorAll('button');
            printButtons.forEach(button => {
                if (button.textContent.includes('Print')) {
                    button.addEventListener('click', function(e) {
                        e.preventDefault();
                        try {
                            window.print();
                        } catch (error) {
                            alert('Please use Ctrl+P (Windows) or Cmd+P (Mac) to print this document.');
                        }
                    });
                }
            });
        };

        // Auto-save checklist progress to localStorage (if available)
        document.addEventListener('DOMContentLoaded', function() {
            const checkboxes = document.querySelectorAll('input[type="checkbox"]');
            
            // Only use localStorage if available (not in Claude artifacts)
            const storageAvailable = typeof(Storage) !== "undefined" && window.localStorage;
            
            checkboxes.forEach(checkbox => {
                if (storageAvailable) {
                    // Load saved state
                    const saved = localStorage.getItem(checkbox.id);
                    if (saved === 'true') {
                        checkbox.checked = true;
                    }
                    
                    // Save on change
                    checkbox.addEventListener('change', function() {
                        localStorage.setItem(this.id, this.checked);
                    });
                }
            });
        });

        // Enhanced print functionality
        function printDocument() {
            // Ensure all content is visible before printing
            const allElements = document.querySelectorAll('.chapter, .toc, .cover-page');
            allElements.forEach(el => {
                el.style.pageBreakInside = 'avoid';
            });
            
            // Add print-specific styles
            const printStyles = document.createElement('style');
            printStyles.innerHTML = `
                @media print {
                    body { 
                        font-size: 12pt !important; 
                        line-height: 1.4 !important;
                        color: black !important;
                        background: white !important;
                    }
                    .container { 
                        box-shadow: none !important; 
