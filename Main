    <script>
        // Add some interactive fun!
        document.addEventListener('DOMContentLoaded', function() {
            // Add click effects to step cards
            const stepCards = document.querySelectorAll('.step-card');
            stepCards.forEach(card => {
                card.addEventListener('click', function() {
                    this.style.transform = 'scale(1.1)';
                    setTimeout(() => {
                        this.style.transform = 'scale(1)';
                    }, 200);
                });
            });

            // Add celebratory animation when scrolling to certificate
            const certificate = document.querySelector('.certificate');
            const observer = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.style.animation = 'bounce 1s ease-in-out';
                    }
                });
            });
            
            if (certificate) {
                observer.observe(certificate);
            }

            // Add floating animation to emojis
            const floatingElements = document.querySelectorAll('.floating');
            floatingElements.forEach((element, index) => {
                element.style.animationDelay = `${index * 0.5}s`;
            });

            // Emergency practice mode
            window.practiceEmergency = function(type) {
                let message = '';
                switch(type) {
                    case 'fire':
                        message = 'üî• FIRE DRILL! Remember: STOP, DROP & GO! üèÉ‚Äç‚ôÇÔ∏è';
                        break;
                    case 'earthquake':
                        message = 'üåç EARTHQUAKE DRILL! Remember: DUCK, COVER & HOLD! ü¶Ü';
                        break;
                    case 'home':
                        message = 'üè† HOME SAFETY DRILL! Remember: SECURE & CALL! üîí';
                        break;
                    default:
                        message = 'ü¶∏‚Äç‚ôÄÔ∏è Great job practicing, Safety Hero! üåü';
                }
                
                alert(message);
                
                // Add some visual celebration
                document.body.style.background = 'linear-gradient(45deg, #ff6b6b, #4ecdc4, #45b7d1, #96ceb4)';
                setTimeout(() => {
                    document.body.style.background = 'linear-gradient(135deg, #667eea 0%, #764ba2 100%)';
                }, 2000);
            };

            // Add practice buttons
            const practiceSection = document.querySelector('.practice-section');
            if (practiceSection) {
                const buttonContainer = document.createElement('div');
                buttonContainer.style.textAlign = 'center';
                buttonContainer.style.marginTop = '30px';
                
                const fireBtn = document.createElement('button');
                fireBtn.className = 'fun-button';
                fireBtn.innerHTML = 'üî• Practice Fire Drill';
                fireBtn.onclick = () => practiceEmergency('fire');
                
                const earthquakeBtn = document.createElement('button');
                earthquakeBtn.className = 'fun-button';
                earthquakeBtn.innerHTML = 'üåç Practice Earthquake Drill';
                earthquakeBtn.onclick = () => practiceEmergency('earthquake');
                
                const homeBtn = document.createElement('button');
                homeBtn.className = 'fun-button';
                homeBtn.innerHTML = 'üè† Practice Home Safety';
                homeBtn.onclick = () => practiceEmergency('home');
                
                buttonContainer.appendChild(fireBtn);
                buttonContainer.appendChild(earthquakeBtn);
                buttonContainer.appendChild(homeBtn);
                practiceSection.appendChild(buttonContainer);
            }
        });

        // Smooth scrolling for better experience
        document.documentElement.style.scrollBehavior = 'smooth';
    </script>
