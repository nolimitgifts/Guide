    <script>
        let completedCategories = new Set();

        function toggleCategory(categoryNum) {
            const content = document.getElementById(`content${categoryNum}`);
            const button = document.querySelector(`#category${categoryNum} .expand-btn`);
            
            if (content.classList.contains('expanded')) {
                content.classList.remove('expanded');
                button.textContent = '+';
            } else {
                content.classList.add('expanded');
                button.textContent = '‚àí';
                
                // Mark as completed
                completedCategories.add(categoryNum);
                updateProgress();
            }
        }

        function updateProgress() {
            const total = 15;
            const completed = completedCategories.size;
            const percentage = Math.round((completed / total) * 100);
            
            // Update progress bar
            const progressBar = document.getElementById('progressBar');
            progressBar.style.width = `${percentage}%`;
            
            // Update progress text
            const progressText = document.getElementById('progressText');
            progressText.textContent = `${completed} of ${total} categories completed (${percentage}%)`;
            
            // Update category pills
            completedCategories.forEach(categoryNum => {
                const pill = document.querySelector(`[data-category="${categoryNum}"]`);
                if (pill) {
                    pill.classList.add('completed');
                }
            });
        }

        // Initialize progress
        updateProgress();

        // Add click handlers to category pills
        document.querySelectorAll('.category-pill').forEach(pill => {
            pill.addEventListener('click', function() {
                const categoryNum = this.dataset.category;
                const categoryCard = document.getElementById(`category${categoryNum}`);
                if (categoryCard) {
                    categoryCard.scrollIntoView({ behavior: 'smooth' });
                    // Auto-expand if not already expanded
                    const content = document.getElementById(`content${categoryNum}`);
                    if (content && !content.classList.contains('expanded')) {
                        toggleCategory(parseInt(categoryNum));
                    }
                }
            });
        });
    </script>
